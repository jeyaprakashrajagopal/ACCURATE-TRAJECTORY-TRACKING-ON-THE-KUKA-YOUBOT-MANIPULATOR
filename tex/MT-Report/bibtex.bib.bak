@Article{826954,
  Title                    = {Stable computed-torque control of robot manipulators via fuzzy self-tuning},
  Author                   = {M. A. Llama and R. Kelly and V. Santibanez},
  Journal                  = {IEEE Transactions on Systems, Man, and Cybernetics, Part B (Cybernetics)},
  Year                     = {2000},

  Month                    = {Feb},
  Number                   = {1},
  Pages                    = {143-150},
  Volume                   = {30},

  Abstract                 = {Computed-torque control is a well-known motion control strategy for manipulators which ensures global asymptotic stability for fixed symmetric positive definite (proportional and derivative) gain matrices. In this paper, we show that global asymptotic stability also holds for a class of gain matrices depending on the manipulator state. This feature increases the potential of the computed-torque control scheme to handle practical constraint in actual robots such as presence of friction in the joints and actuators with limited torque capabilities. We illustrate this potential by means of a fuzzy self-tuning algorithm to select the proportional and derivative gains according to the actual tracking position error. Experiments on a two degrees of freedom robot arm show the usefulness of the proposed approach},
  Doi                      = {10.1109/3477.826954},
  ISSN                     = {1083-4419},
  Keywords                 = {adaptive control;asymptotic stability;fuzzy control;manipulators;matrix algebra;motion control;self-adjusting systems;actuators;fuzzy self-tuning;fuzzy self-tuning algorithm;gain matrices;global asymptotic stability;motion control strategy;proportional and derivative gains;robot arm;robot manipulators;stable computed-torque control;Asymptotic stability;Friction;Fuzzy control;Manipulators;Motion control;PD control;Proportional control;Robot control;Symmetric matrices;Torque control}
}

